@{
    ViewBag.Title = "Index";
}

<h2>Upload File</h2>

@using (Html.BeginForm("Index", "File", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
            <input type="file" name="file" />
    </div>
    <div class="form-group">
        <div class="btn btn-default">
            <input type="submit" value="OK" />
        </div>
    </div>
}
